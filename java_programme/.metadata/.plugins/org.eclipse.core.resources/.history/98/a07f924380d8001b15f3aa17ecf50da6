package helloworld.wyyadd.test;

public class Parrot extends Bird {
	public String name = "Parrot";
	public void Tweet() {
		System.out.println("Hello!");
	}
	

	public void Eat() {
		System.out.println("Parrot Eat!");
	}
	public static void main(String []args) {
		Parrot p = new Parrot();
		Bird b = p;
		System.out.println(p.name + b.name);
		b.Fly();
	}
}
