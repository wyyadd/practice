package helloworld.wyyadd.test;

public class Parrot extends Bird {
	public void Tweet() {
		System.out.println("Hello!");
	}
	
	public void Fly() {
		System.out.println("Parrot fly");
	}

	public void Eat() {
		System.out.println("Parrot Eat!");
	}
	public static void main(String []args) {
		Parrot p = new Parrot();
		p.Fly();
		p.Tweet();
		((Bird)p).Tweet();
		//p.Eat();
	}


}
